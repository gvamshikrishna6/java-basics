package mktjavaoperators;

public class Logicaloperators
{

	public static void main(String[] args)
	{
		int age=19;
		char gender='M';
		if(age>=18 && gender=='M')
		{
			System.out.println("FULL TICKET");
		}
		if(age>=18 || gender=='F')
				{
			System.out.println("STATEMENT 2");
				}		
				
			
		}
		
		
	}

}